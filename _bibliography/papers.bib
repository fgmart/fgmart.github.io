@article{TosaMarti2010ia,
  author = {Sachiko Tosa* and Fred Martin},
  title = {Impact of a professional development program using data-loggers on science teachersâ€™ attitudes towards inquiry-based teaching},
  journal = {Journal of Computers in Mathematics and Science Teaching},
  volume = {29},
  number = {3},
  year = {2010},
  month = {8},
  pages = {303--325},
  address = {Waynesville, NC USA},
  publisher = {Association for the Advancement of Computing in Education (AACE)},
