@inproceedings{6556350,
  author={Zhang*, Haiyang and Martin, Fred},
  booktitle={2013 IEEE Conference on Technologies for Practical Robot Applications (TePRA)}, 
  title={CUDA accelerated robot localization and mapping}, 
  year={2013},
  volume={},
  number={},
  pages={1-6},
  keywords={Graphics processing units;Algorithm design and analysis;Acceleration;Simultaneous localization and mapping;Benchmark testing;Optimization;robot;localization;mapping;SLAM;GPU;GPGPU;parallel;CUDA},
  doi={10.1109/TePRA.2013.6556350},
  abstract = {We present a method to accelerate robot
localization and mapping by using CUDA (Compute Unified
Device Architecture), the general purpose parallel computing
platform on NVIDIA GPUs. In robotics, the particle filter-based
SLAM (Simultaneous Localization and Mapping) algorithm has
many applications, but is computationally intensive. Prior work
has used CUDA to accelerate various robot applications, but
particle filter-based SLAM has not been implemented on CUDA
yet. Because computations on the particles are independent of
each other in this algorithm, CUDA acceleration should be highly
effective. We have implemented the SLAM algorithm's most time
consuming step, particle weight calculation, and optimized
memory access by using texture memory to alleviate memory
bottleneck and fully leverage the parallel processing power. Our
experiments have shown the performance has increased by an
order of magnitude or more. The results indicate that oftloading
to GPU is a cost-effective way to improve SLAM algorithm
performance.},
  html = {https://ieeexplore.ieee.org/abstract/document/6556350},
  pdf = {CUDA_accelerated_robot_localization_and_mapping.pdf},
}

