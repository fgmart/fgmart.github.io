@inproceedings{melanson2018redefining,
  title={Redefining Science Learning with a Platform for Sharing Experimental Data},
  author={Melanson*, Maureen and Javier*, Glenda and Canane*, Caitlin and Shaheen*, Minda},
  booktitle={Society for Information Technology \& Teacher Education International Conference},
  pages={1941--1947},
  year={2018},
